{
    "require": {
        "php": ">=8.0.2",
        "ext-mbstring": "*",
        "symfony/dependency-injection": "^6.0",
        "symfony/console": "^6.0",
        "symfony/config": "^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "^9.5"
    },
    "scripts": {
        "app:example": "bin/console app:example 'John Doe'",
        "test": "vendor/bin/phpunit"
